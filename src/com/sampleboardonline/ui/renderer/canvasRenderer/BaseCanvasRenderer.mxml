<renderer:BaseRenderer
        xmlns:fx="http://ns.adobe.com/mxml/2009"
        xmlns:renderer="com.sampleboardonline.ui.renderer.*"
        width="@{itemVO.canvasGeometry.width}"
        height="@{itemVO.canvasGeometry.height}"
        x="@{itemVO.canvasGeometry.x}"
        y="@{itemVO.canvasGeometry.y}"
        scaleY="@{itemVO.canvasGeometry.scaleY}"
        scaleX="@{itemVO.canvasGeometry.scaleY}"
        rotation="@{itemVO.canvasGeometry.rotation}">

    <fx:Script><![CDATA[
        import com.sampleboardonline.model.vo.CanvasItemVO;

        [Bindable(event="dataChange")]
        protected function get itemVO():CanvasItemVO {
            return data as CanvasItemVO;
        }
    ]]></fx:Script>

</renderer:BaseRenderer>
